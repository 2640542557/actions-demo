{
	"title": "帮助中心",
	"plugins": ["-lunr", "-search", "search-plus","chapter-fold","hide-element",
				"splitter","copy-code-button","anchor-navigation-ex",
				"lightbox","tbfed-pagefooter","pageview-count-hc",
				"-sharing", "sharing-plus-hc"],
	"pluginsConfig": {
        "hide-element": {
            "elements": [".gitbook-link"]
        },
		"lightbox": {
			"includeJQuery": false,
			"sameUuid": true,
			"options": {
				"resizeDuration": 100,
				"wrapAround": false
			}
		},
		"tbfed-pagefooter": {
            "copyright":"Copyright © yqsh 2023",
            "modify_label": "更新时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
		"sharing": {
			"douban": false,
			"shareUrl": true,
			"facebook": false,
			"google": false,
			"pocket": false,
			"qq": false,
			"qzone": false,
			"twitter": false,
			"weibo": true,
			"all": []
		}
    }
}